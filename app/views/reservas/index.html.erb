<%- model_class = Reserva -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:data_inicial) %></th>
      <th><%= model_class.human_attribute_name(:quilometragem_inicial) %></th>
      <th><%= model_class.human_attribute_name(:nivel_tanque) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
     
    <tr>
      <td><%= current_usuario.reserva.id %></td>
      <td><%= current_usuario.reserva.data_inicial %></td>
      <td><%= current_usuario.reserva.quilometragem_inicial %></td>
      <td><%= current_usuario.reserva.nivel_tanque %></td>
      <td><%=l current_usuario.reserva.created_at %></td>
      <td>
        <%= link_to t('.show', :default => t("helpers.links.show")),
        reserva_path(current_usuario.reserva), :class => 'btn btn-default btn-xs' %>
        <!--%= link_to t('.devolver', :default => t("helpers.links.edit")),
        new_devolucao_path, :class => 'btn btn-default btn-xs' %-->
        <!--%= link_to t('.destroy', :default => t("helpers.links.destroy")),
        reserva_path(current_usuario.reserva),
        :method => :delete,
        :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
        :class => 'btn btn-xs btn-danger' %-->
      </td>
    </tr>

  </tbody>
</table>

