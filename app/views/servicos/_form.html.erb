<%= simple_form_for @servico, :html => { :class => 'form-horizontal' } do |f| %>
  <%= f.input :abastecimento %>
  <%= error_span(@servico[:abastecimento]) %>
  <%= f.input :tipo %>
  <%= error_span(@servico[:tipo]) %>

  <%= f.button :submit, :class => 'btn-primary' %>
  <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                servicos_path, :class => 'btn btn-default' %>
<% end %>
